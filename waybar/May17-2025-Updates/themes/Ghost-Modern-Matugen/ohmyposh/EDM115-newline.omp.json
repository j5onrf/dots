{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
"palette": {
  "user_fg": "p:color15",
  "path_fg": "p:color10",
  "text_accent_fg": "p:color15",
  "git_fg": "p:color13",
  "git_branch_icon_fg": "p:color13",
  "prompt_char_fg": "p:color10"
},
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:user_fg",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "p:path_fg",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<p:text_accent_fg>in</> {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:git_fg",
          "properties": {
            "branch_icon": " <p:git_branch_icon_fg><b>\ue0a0 </b></>",
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": "<p:text_accent_fg>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:prompt_char_fg",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true, // This was already here, adds space AFTER command output
  "version": 3
}
