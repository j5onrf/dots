{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
   "logo": {
    "type": "chafa", // Force Chafa for character art
    "source": "~/.config/fastfetch/logos/current_random_themed_logo.png", // script saves the output
	"width": 30,
    "preserveAspectRatio": true, // Optional, usually good
	"padding": {
	"top": 3,
	"left": 2,
	},
    },
    "display": {
        "separator": " "
    },
    "modules": [
        "break",
                {
            "type": "title",
            "key": "󰣇 ",
			"format": "Arch",
            "keyColor": "blue"
        },
        {
            "type": "kernel",
            "key": " ",
            "format": "{2}",
            "keyColor": "yellow"
        },
        {
            "type": "wm",
            "key": " ",
            "format": "{2} {5}",          
            "keyColor": "blue"
        },
        {
            "type": "shell",
            "key": " ",
            "keyColor": "yellow"
        },
        {
            "type": "terminal",
            "key": " ",
            "keyColor": "blue"
        },
        {
            "type": "packages",
            "key": "󰏖 ",
            "format": "{1} packages",
            "keyColor": "yellow"
        },
                {
            "type": "memory",
            "key": "󰍛 ",
            "keyColor": "yellow",
            // format: used / total
            "format": "{1}"
        },
        {
            "type": "uptime",
            "key": "󰔛 ",
            "format": "{1} days {2} hours",
            "keyColor": "blue",
        },
                {
            "type": "custom",
            "key": "  ",
            "format": "\u001b[31m⬤ \u001b[32m⬤ \u001b[33m⬤ \u001b[34m⬤ \u001b[35m⬤ \u001b[36m⬤ \u001b[0m"
        },
		"break",
    ]
}
